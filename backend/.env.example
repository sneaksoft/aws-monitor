# Application
APP_ENV=development
DEBUG=true
LOG_LEVEL=INFO

# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/aws_monitor

# Redis
REDIS_URL=redis://localhost:6379/0
CACHE_TTL=300

# AWS
AWS_REGION=us-east-1
# AWS_ROLE_ARN=arn:aws:iam::123456789012:role/aws-monitor-role
# AWS_EXTERNAL_ID=

# Cognito (optional for development)
# COGNITO_USER_POOL_ID=
# COGNITO_CLIENT_ID=
# COGNITO_REGION=us-east-1

# Security
# ADMIN_OVERRIDE_CODE=your-secret-code
PROTECTED_TAGS=production,prod,critical

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:5173
